<!--
  max   : number of items displayed for each feed.  (can be set globally and/or per feed)
  icon  : emoji or unicode character. used as favicon and logo. (e.g. 🌍 📰 👓 🛸 🛰️ 🚀)
  color : css file from ./assets/color/  (blue.css, green.css, flat.css or solarized.css)
  theme : css file from ./assets/theme/ (101010.css, planet.css, reader.css or slash.css)
-->
<config max="10" icon="🛸" color="flat.css" theme="slash.css" beta="true" path="../assets">
  <feed max="1">https://xkcd.com/rss.xml</feed>
  <feed>http://rss.slashdot.org/Slashdot/slashdotMain</feed>
  <feed>https://github.blog/feed/</feed>
  <feed>https://netflixtechblog.com/feed</feed>
  <feed>https://www.linux.com/feed/</feed>
</config>
